<script language = "JavaScript" type = "text/javascript">
function load() {
   alert(Session["isLogin"]);
   if (!((Boolean)session.getAttribute("isLogin"))){
     window.location.href = "http://www.google.ca";
   }else{
     alert(session.getAtrribute("USERNAME"));
   }
}

</script>
</head>
<body onload="load()">
